#include <iostream>
#include <sstream>
#include <iomanip>
#include <cmath>
using namespace std;
int main()
{
    int N;
    cin >> N;
    string line;
    float *n= new float[N] ,fault, student[10] = { 12.7, 4.3 , 3.2,2.8,2.6,2.4,2.4,2.3, 2.3 }, *ai= new float[N], a , *detai= new float[N], *sqdetai = new float[N], om, P, detarand;
    for (int i = 0; i < N; i++) {
        cout << "a" << i + 1 << ": ";
        while (getline(cin, line)) {
            stringstream ss(line);
            if ((ss >> n[i]) and (n[i] >= 0))
            {
                ss >> n[i];
                if (ss.eof())
                    break;
            }
        }
    }
    float avg =0;
    for (int i = 0; i < N; i++) {
        avg = avg + n[i];
    }
    avg = avg / N;
    for (int i = 0; i < N; i++) {
        detai[i]= abs(n[i]-avg);
    }
    float sqavg= 0;
    for (int i = 0; i < N; i++) {
        sqdetai[i] =detai[i]*detai[i];
        sqavg = sqavg+ detai[i] * detai[i];
    }
    sqavg = sqrt(sqavg / (N * (N - 1)));
    P = 0.95;
    fault = sqavg * student[N-2];
    cout << setw(15) << "№" << setw(15) << "ai" << setw(15) << "a" << setw(15) << "det(ai)" << setw(15) << "det(ai)^2" << setw(15) << "om" << setw(15) << "P" << setw(5) << "tpn" << setw(15) << "det(a)сл" << endl;
    for (int i = 0; i < N; i++) {
        cout << setw(15) << i+1 << setw(15) << n[i] << setw(15) << avg << setw(15) << detai[i] << setw(15) << sqdetai[i]  << setw(15) << sqavg << setw(15) << P << setw(15) << student[N-2] << setw(15) << fault << endl;
    }

}
